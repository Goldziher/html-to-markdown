{
  "name": "@html-to-markdown/node",
  "version": "2.2.0",
  "description": "High-performance HTML to Markdown converter - Node.js native bindings",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "https://github.com/Goldziher/html-to-markdown",
  "license": "MIT",
  "keywords": [
    "html",
    "markdown",
    "converter",
    "rust",
    "napi",
    "native"
  ],
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "napi": {
    "name": "html-to-markdown-node",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-apple-darwin",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf"
      ]
    }
  },
  "engines": {
    "node": ">= 10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "node test.js",
    "test:vitest": "vitest run",
    "test:vitest:watch": "vitest",
    "universal": "napi universal",
    "version": "napi version",
    "clean": "rm -rf dist node_modules *.node"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.3.1"
  },
  "optionalDependencies": {
    "@html-to-markdown/node-darwin-arm64": "2.2.0",
    "@html-to-markdown/node-darwin-x64": "2.2.0",
    "@html-to-markdown/node-linux-arm-gnueabihf": "2.2.0",
    "@html-to-markdown/node-linux-arm64-gnu": "2.2.0",
    "@html-to-markdown/node-linux-arm64-musl": "2.2.0",
    "@html-to-markdown/node-linux-x64-gnu": "2.2.0",
    "@html-to-markdown/node-linux-x64-musl": "2.2.0",
    "@html-to-markdown/node-win32-arm64-msvc": "2.2.0",
    "@html-to-markdown/node-win32-x64-msvc": "2.2.0"
  },
  "dependencies": {
    "@emnapi/runtime": "1.5.0",
    "@octokit/core": "7.0.5",
    "@types/node": "24.7.2",
    "typanion": "3.14.0"
  }
}
